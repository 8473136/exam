<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.guozhi.mapper.MenuMapper">
    <select id="getMenusOrder" resultType="com.guozhi.dto.MenuDTO">
        select * from tab_sys_menu  where is_deleted = 0 order by `menu_order` asc
    </select>

</mapper>