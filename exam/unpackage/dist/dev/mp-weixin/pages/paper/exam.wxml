<view class="bg"><view class="paper_info"><view class="time"><view class="count_down"><image class="count_down_icon" src="../../static/images/time.png"></image><view class="count_down_text"><uni-countdown vue-id="aaa56d2a-1" show-day="{{false}}" background-color="#feebed" minute="{{paper_info.examDuration}}" data-event-opts="{{[['^timeup',[['timeup']]]]}}" bind:timeup="__e" bind:__l="__l"></uni-countdown></view></view></view><view class="paper_title">{{paper_info.paperName}}</view><view class="exam_time">{{"考试时间："+paper_info.startTime+" - "+paper_info.endTime}}</view><view class="paper_describe">{{paper_info.paperContent}}</view></view><swiper class="swiper_group" current="{{questionCurrent}}" data-event-opts="{{[['change',[['changeQuestion',['$event']]]]]}}" bindchange="__e"><block wx:for="{{paper_info.questionDTOS}}" wx:for-item="item" wx:for-index="questionIndex" wx:key="questionIndex"><swiper-item class="swiper-item"><view class="question_item"><view class="question_header"><uni-tag class="questino_type" vue-id="{{'aaa56d2a-2-'+questionIndex}}" text="{{item.questionTypeChinese}}" size="small" circle="{{true}}" type="primary" bind:__l="__l"></uni-tag><uni-tag class="question_count" vue-id="{{'aaa56d2a-3-'+questionIndex}}" text="{{questionIndex+1+'/'+paper_info.questionDTOS.length}}" size="small" inverted="{{true}}" type="primary" bind:__l="__l"></uni-tag></view><view class="question_title">{{''+item.questionName+''}}</view><block wx:for="{{item.options}}" wx:for-item="option" wx:for-index="index"><view class="options"><block wx:if="{{item.questionType=='TIANKONGTI'}}"><input class="option" type="text"/></block><block wx:else><view data-event-opts="{{[['tap',[['optionClick',[questionIndex,index,'$0','$1'],[[['paper_info.questionDTOS','',questionIndex]],[['paper_info.questionDTOS','',questionIndex],['options','',index]]]]]]]}}" class="{{['option',(option.check)?'option_selected':'']}}" bindtap="__e">{{option.optionContent}}</view></block></view></block></view></swiper-item></block></swiper><view class="control_button"><button class="prev" type="primary" data-event-opts="{{[['tap',[['prev',['$event']]]]]}}" bindtap="__e">上一题</button><block wx:if="{{paper_info.questionDTOS&&questionCurrent<paper_info.questionDTOS.length-1}}"><button class="next" type="primary" data-event-opts="{{[['tap',[['next',['$event']]]]]}}" bindtap="__e">下一题</button></block><block wx:if="{{paper_info.questionDTOS&&questionCurrent==paper_info.questionDTOS.length-1}}"><button class="submit" type="primary" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交</button></block></view></view>